                {
                    "name": "proposal_reference",
                    "description": "Dynamic box ref per proposal account address"
                },
                {
                    "name": "proposals",
                    "description": "Proposals box reference for proposal contract IDs"
                }
            ],
            "returns": {
                "type": "uint64"
            }
